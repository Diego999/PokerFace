
package ch.hearc.coursjava.poo.thread.additionvectorielle;



public class Additionneur
	{

	/*------------------------------------------------------------------*\
	|*							Constructeurs							*|
	\*------------------------------------------------------------------*/

	public Additionneur(long[] tab1, long[] tab2)
		{
		this.tab1 = tab1;
		this.tab2 = tab2;
		}

	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	/*------------------------------*\
	|*				Set				*|
	\*------------------------------*/

	/*------------------------------*\
	|*				Get				*|
	\*------------------------------*/

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	/*------------------------------------------------------------------*\
	|*							Attributs Private						*|
	\*------------------------------------------------------------------*/
	//Inputs
	private long[] tab1;
	private long[] tab2;

	//Outputs
	private long[] tabResult;
	}

