
package ch.hearc.coursjava.poo.thread.additionvectorielle;



public class Interval
	{

	/*------------------------------------------------------------------*\
	|*							Constructeurs							*|
	\*------------------------------------------------------------------*/

	public Interval(int iStart, int iStop)
		{
		super();
		this.iStart = iStart;
		this.iStop = iStop;
		}

	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	/*------------------------------*\
	|*				Set				*|
	\*------------------------------*/

	/*------------------------------*\
	|*				Get				*|
	\*------------------------------*/	
	
	public int getiStart()
		{
		return this.iStart;
		}
	
	public int getiStop()
		{
		return this.iStop;
		}

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	/*------------------------------------------------------------------*\
	|*							Attributs Private						*|
	\*------------------------------------------------------------------*/
	//Inputs
	private int iStart;
	private int iStop;

	}

