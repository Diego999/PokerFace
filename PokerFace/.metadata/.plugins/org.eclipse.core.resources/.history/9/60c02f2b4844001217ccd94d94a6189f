
package ch.hearc.coursjava.poo.thread.additionvectorielle;

public class UseAdditionneur
	{

	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	public static void main(String[] args)
		{
		main();
		}

	public static void main()
		{
		int n = Integer.MAX_VALUE;
		long[] tab1 = new long[n];

		for(int i = 1; i <= n; i++)
			{
			tab1[i-1] = i;
			}
		}

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	}
